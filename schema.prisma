generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  GUEST
  HOST
  ADMIN
}

enum KycStatus {
  PENDING
  APPROVED
  REJECTED
}

enum DocType {
  V5C
  INSURANCE
  MOT
  ROAD_TAX
  DRIVER_LICENSE
  OTHER
}

enum DocStatus {
  UPLOADED
  VERIFIED
  REJECTED
  EXPIRED
}

enum VehicleStatus {
  DRAFT
  ACTIVE
  SUSPENDED
}

enum BookingStatus {
  QUOTED
  PENDING
  CONFIRMED
  CANCELED
  COMPLETED
  DISPUTED
}

enum PayoutStatus {
  CREATED
  PAID
  FAILED
  CANCELED
}


